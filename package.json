{
    "name": "donner",
    "version": "1.1.0",
    "description": "donner fires dom events on en element. blitz handles events",
    "umd:main": "lib/donner.umd.js",
    "module": "lib/donner.es.js",
    "main": "lib/donner.js",
    "scripts": {
        "test": "ava",
        "trash": "rimraf lib",
        "lint": "eslint --fix --format table ./src",
        "rollup": "rollup -c",
        "gh-pages": "gh-pages -d ./docs",
        "watch": "rollup -cwm",
        "build": "run-s trash rollup minify:* size",
        "minify:cjs": "uglifyjs lib/donner.umd.js --config-file ./uglify.json -o lib/donner.min.js",
        "size": "echo \"Gzipped Size: $(strip-json-comments lib/donner.js | gzip-size)\"",
        "build:micro": "run-s trash && microbundle build -i src/index.js",
        "build:brsfy": "run-s trash && browserify -r ./src --debug -o lib/donner.bundle.js",
        "build:brsfy:flat": "run-s trash && browserify ./src/donner.js | browser-unpack | browser-pack-flat",
        "build:parcel": "run-s trash && parcel build src/donner.js --public-url ./ -d lib"
    },
    "files": [
        "lib"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/soenkekluth/donner.git"
    },
    "keywords": [],
    "author": "SÃ¶nke Kluth <mail@soenkekluth.com> (https://soenkekluth.com)",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/soenkekluth/donner/issues"
    },
    "homepage": "https://github.com/soenkekluth/donner#readme",
    "bundlesize": [
        {
            "path": "./lib/donner.js",
            "threshold": "1Kb"
        }
    ],
    "devDependencies": {
        "ava": "^0.25.0",
        "babelify": "^8.0.0",
        "browser-pack-flat": "^3.0.8",
        "browser-unpack": "^1.2.0",
        "browserify": "^16.1.0",
        "bundlesize": "^0.16.0",
        "eslint": "^4.18.2",
        "gh-pages": "^1.1.0",
        "gzip-size-cli": "^2.1.0",
        "jest": "^22.4.2",
        "jsdom": "^11.6.2",
        "microbundle": "^0.4.4",
        "npm-run-all": "^4.1.2",
        "parcel-bundler": "^1.6.2",
        "rimraf": "^2.6.2",
        "rollup": "^0.56.3",
        "rollup-plugin-buble": "^0.19.2",
        "rollup-plugin-commonjs": "^8.3.0",
        "rollup-plugin-node-resolve": "^3.0.3",
        "rollup-plugin-replace": "^2.0.0",
        "uglify-js": "^3.3.12"
    },
    "dependencies": {
        "eslint-config-sk": "file:.yalc/eslint-config-sk",
        "setimmediate": "^1.0.5"
    }
}
